void task1()
{
	long int i;
	while(1)
	{
		Bit1_PutVal(0);
		for(i=0;i<0x7ffff;i++);
		Bit1_PutVal(1);
		for(i=0;i<0x7ffff;i++);
	}
}

void task2()
{
	long int i;
	while(1)
	{
		Bit2_PutVal(0);
		for(i=0;i<0x7ffff;i++);
		Bit2_PutVal(1);
		for(i=0;i<0x7ffff;i++);
	}
}

void task3()
{
	long int i;
	while(1)
	{
		Bit3_PutVal(0);
		for(i=0;i<0x7ffff;i++);
		Bit3_PutVal(1);
		for(i=0;i<0x7ffff;i++);
	}
}

